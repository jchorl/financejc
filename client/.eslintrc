{
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "gapi": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "strict": "off",
    "no-debugger": "warn",
    "no-console": "warn",
    "object-curly-spacing": ["error", "always"],
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"]
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"]
}
